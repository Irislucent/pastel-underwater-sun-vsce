        "tokenColors": [
                {
                        "name": "Comments",
                        "scope": [
                                "comment",
                                "punctuation.definition.comment"
                        ],
                        "settings": {
                                "fontStyle": "italic",
                                "foreground": "#9E9A95d0"
                        }
                },
                {
                        "name": "Comments: Preprocessor",
                        "scope": "comment.block.preprocessor",
                        "settings": {
                                "fontStyle": "",
                                "foreground": "#9E9A95d0"
                        }
                },
                {
                        "name": "Comments: Documentation",
                        "scope": [
                                "comment.documentation",
                                "comment.block.documentation"
                        ],
                        "settings": {
                                "foreground": "#9E9A95d0"
                        }
                },
                {
                        "name": "Invalid - Illegal",
                        "scope": "invalid.illegal",
                        "settings": {
                                "background": "#8F5652"
                        }
                },
                {
                        "name": "Operators",
                        "scope": "keyword.operator",
                        "settings": {
                                "foreground": "#8d5093e0"
                        }
                },
                {
                        "name": "Keywords",
                        "scope": [
                                "keyword",
                                "storage"
                        ],
                        "settings": {
                                "foreground": "#de6f6fe0",
                                "fontStyle": "bold"
                        }
                },
                {
                        "name": "Types",
                        "scope": [
                                "storage.type",
                                "support.type"
                        ],
                        "settings": {
                                "foreground": "#c981bee0"
                        }
                },
                {
                        "name": "Language Constants",
                        "scope": [
                                "constant.language",
                                "support.constant",
                                "variable.language"
                        ],
                        "settings": {
                                "foreground": "#5aa19cf0"
                        }
                },
                {
                        "name": "Variables",
                        "scope": [
                                "variable",
                                "support.variable"
                        ],
                        "settings": {
                                "foreground": "#605a52"
                        }
                },
                {
                        "name": "Self",
                        "scope": [
                                "variable.parameter.function.language.special.self",
                        ],
                        "settings": {
                                "foreground": "#6172b8e0"
                        }
                },
                {
                        "name": "Properties",
                        "scope": [
                                "variable.other.property",
                        ],
                        "settings": {
                                "foreground": "#605a52"
                        }
                },
                {
                        "name": "Functions",
                        "scope": [
                                "entity.name.function",
                                "support.function"
                        ],
                        "settings": {
                                "foreground": "#605a52"
                        }
                },
                {
                        "name": "Classes",
                        "scope": [
                                "entity.name.type",
                                "entity.other.inherited-class",
                                "support.class"
                        ],
                        "settings": {
                                // "fontStyle": "italic",
                                "foreground": "#c98b2fe0"
                        }
                },
                {
                        "name": "Exceptions",
                        "scope": "entity.name.exception",
                        "settings": {
                                "fontStyle": "italic bold",
                                "foreground": "#605a52"
                        }
                },
                {
                        "name": "Sections",
                        "scope": "entity.name.section",
                        "settings": {
                                "fontStyle": "bold",
                                "foreground": "#83577D"
                        }
                },
                {
                        "name": "Numbers, Characters",
                        "scope": [
                                "constant.numeric",
                                "constant.character",
                                "constant"
                        ],
                        "settings": {
                                "foreground": "#959b4de0"
                        }
                },
                {
                        "name": "Strings",
                        "scope": "string",
                        "settings": {
                                "foreground": "#749B4De0"
                        }
                },
                {
                        "name": "Strings: Escape Sequences",
                        "scope": "constant.character.escape",
                        "settings": {
                                "foreground": "#5aa19ce0"
                        }
                },
                {
                        "name": "Strings: Regular Expressions",
                        "scope": "string.regexp",
                        "settings": {
                                "foreground": "#886A44"
                        }
                },
                {
                        "name": "Strings: Symbols",
                        "scope": "constant.other.symbol",
                        "settings": {
                                "foreground": "#5aa19ce0"
                        }
                },
                {
                        "name": "Punctuation",
                        "scope": "punctuation",
                        "settings": {
                                "foreground": "#bf3d9ae0"
                        }
                },
                {
                        "name": "HTML: Doctype Declaration",
                        "scope": [
                                "meta.tag.sgml.doctype",
                                "meta.tag.sgml.doctype string",
                                "meta.tag.sgml.doctype entity.name.tag",
                                "meta.tag.sgml punctuation.definition.tag.html"
                        ],
                        "settings": {
                                "foreground": "#9E9A95"
                        }
                },
                {
                        "name": "HTML: Tags",
                        "scope": [
                                "meta.tag",
                                "punctuation.definition.tag.html",
                                "punctuation.definition.tag.begin.html",
                                "punctuation.definition.tag.end.html"
                        ],
                        "settings": {
                                "foreground": "#9E9A95"
                        }
                },
                {
                        "name": "HTML: Tag Names",
                        "scope": "entity.name.tag",
                        "settings": {
                                "foreground": "#83577De0",
                                // "fontStyle": ""
                        }
                },
                {
                        "name": "HTML: Attribute Names",
                        "scope": [
                                "meta.tag entity.other.attribute-name",
                                "entity.other.attribute-name.html"
                        ],
                        "settings": {
                                "foreground": "#556995"
                        }
                },
                {
                        "name": "HTML: Entities",
                        "scope": [
                                "constant.character.entity",
                                "punctuation.definition.entity"
                        ],
                        "settings": {
                                "foreground": "#5aa19ce0"
                        }
                },
